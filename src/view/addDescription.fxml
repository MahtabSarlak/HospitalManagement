<AnchorPane prefHeight="420.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddDescriptionController">
    <children>
        <ImageView fitHeight="420.0" fitWidth="450.0">
            <image>
                <Image url="@file/bck3.png" />
            </image>
        </ImageView>
        <GridPane layoutX="25.0" layoutY="41.0" prefHeight="215.0" prefWidth="379.0" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="41.0">
