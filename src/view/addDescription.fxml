            </children>
        </GridPane>
        <Button layoutX="237.0" layoutY="333.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="38.0" prefWidth="74.0" text="Ok">
            <font>
                <Font size="17.0" />
            </font>
        </Button>
        <Button layoutX="330.0" layoutY="333.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel">
            <font>
                <Font size="17.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
